{
  "name": "slack-bot-base",
  "version": "1.0.1",
  "author": "Petar Zarkov",
  "description": "Slack Bot Base for slack utilities",
  "repository": {
    "type": "git",
    "url": "https://github.com/petarzarkov/slack-bot-base.git"
  },
  "engines": {
    "node": ">=15.4.0"
  },
  "main": "./build/index.js",
  "scripts": {
    "clean": "rimraf ./build/**",
    "dev": "nodemon node build/index",
    "start": "node build/index",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w",
    "bnw": "npm run build",
    "bns": "npm run bnw && npm run start",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js --ext .ts,.tsx ./src --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"Warn: no tests yet\" && exit 0",
    "test:coverage": "echo \"Warn: no tests yet\" && exit 0",
    "heroku-prebuild": "npm ci",
    "heroku-cleanup": "npm install --only=prod"
  },
  "dependencies": {
    "@slack/bolt": "3.12.1",
    "hot-utils": "1.4.2",
    "config": "3.3.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/config": "0.0.41",
    "@types/node": "17.0.23",
    "@types/uuid": "8.3.4",
    "@types/node-fetch": "2.6.1",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.13.0",
    "dotenv": "16.0.1",
    "rimraf": "3.0.2",
    "nodemon": "2.0.19",
    "ts-node": "10.7.0",
    "typescript": "4.7.4"
  }
}
